{
  "name": "nacomer-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node -r dotenv/config ./bin/www",
    "dev": "nodemon -r dotenv/config ./bin/www",
    "sequelize": "sequelize",
    "create-db": "sequelize db:create",
    "migrate": "sequelize-cli db:migrate",
    "rollback": "sequelize-cli db:migrate:undo:all",
    "generate-seeds": "sequelize-cli seed:generate",
    "userfile": "sequelize model:generate --name NacomerUser --attributes name:string,password:string",
    "setup": "yarn migrate && yarn seed",
    "lint": "eslint --fix --ext .js .",
    "prettier": "prettier --write ./*/*.js",
    "test": "mocha test && yarn lint && yarn prettier",
    "test-only": "mocha test"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "morgan": "~1.9.1",
    "pg": "^8.5.1",
    "pug": "2.0.0-beta11",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.0",
    "prettier-eslint-cli": "^5.0.0",
    "pretty-quick": "^3.1.0"
  }
}